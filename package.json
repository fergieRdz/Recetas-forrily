{
  "author": "Fernanda Rodriguez",
  "project": "forkify",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run build && npm start",
    "start": "parcel index.html",
    "clean": "del /s /q .parcel-cache dist 2>nul || rmdir /s /q .parcel-cache dist 2>nul || exit 0",
    "build": "npm run clean && parcel build index.html"
  },
  "dependencies": {
    "fraction.js": "^5.3.4",
    "parcel": "^2.16.3"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.16.3",
    "nodemon": "^3.1.7"
  }
}
